<%- include('../partition/header.ejs') %>

<section class="login-wrap mtb-40">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-md-6">
                <div class="login-box">
                    <h1 class="alert alert-success text-center">Display </h1>
                    <table class="table">
                        <thead>
                            
                          <tr>
                            <th scope="col">Id</th>
                            <th scope="col">Name</th>
                            <th scope="col">Designation</th>
                            <th scope="col">Subject</th>
                            <th scope="col">Action</th>
                          </tr>
                        </thead>
                        <tbody>
                            <% var i=1 %>   
                            <% data.forEach((item)=>{ %>
   
                            
                               <tr>
                                   <td><%= i++ %></td>
                                   <td><%= item.name %></td>
                                   <td><%= item.designation %></td>
                                   <td><%= item.subject %></td>
                                   <td>
                                       <a href="/teacher/view/<%=item._id %>">view </a>
                                       <a href="">Edit </a>
                                       <a href="">delete </a>
                                   </td>
                               </tr>
                             <%})%>
                        </tbody>
                      </table>
                </div>         
            </div>
        </div>
    </div>
</section>


<%- include('../partition/footer.ejs') %>